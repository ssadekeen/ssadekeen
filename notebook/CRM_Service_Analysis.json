				"source": [
					"%%pyspark\n",
					"blob_account_name = \"whillake\"\n",
					"blob_container_name = \"whilfs\"\n",
					"from pyspark.sql import SparkSession\n",
					"\n",
					"sc = SparkSession.builder.getOrCreate()\n",
					"token_library = sc._jvm.com.microsoft.azure.synapse.tokenlibrary.TokenLibrary\n",
					"blob_sas_token = token_library.getConnectionString(\"CRMBlobStorage\")\n",
					"\n",
					"spark.conf.set(\n",
					"    'fs.azure.sas.%s.%s.blob.core.windows.net' % (blob_container_name, blob_account_name),\n",
					"    blob_sas_token)\n",
					"df = spark.read.load('wasbs://whilfs@whillake.blob.core.windows.net/OnlieProductRegistration/dboDealer_Location.json', format='json')\n",
					"display(df.limit(10))"
				],
				"attachments": null,
				"execution_count": 1
